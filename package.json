{
  "name": "ideahacks.la",
  "version": "1.0.0",
  "engines": {
    "node": "8.x.x"
  },
  "description": "Website for UCLA's IDEA Hacks",
  "main": "index.js",
  "scripts": {
    "development": "./node_modules/.bin/nodemon server.js",
    "format": "./node_modules/.bin/prettier --single-quote --print-width=120 --no-semi --write ./**.js ./ideahacks/**.js ./ideahacks/**/**.js ./ideahacks/**/**/**.js ./public/css/**.css",
    "format-check": "./node_modules/prettier-check/cli.js --single-quote --print-width=120 --no-semi ./**.js ./ideahacks/**.js ./ideahacks/**/**.js ./ideahacks/**/**/**.js ./public/css/**.css",
    "lint": "./node_modules/.bin/eslint --fix --quiet ./**.js ./ideahacks/**.js ./ideahacks/**/**.js ./ideahacks/**/**/**.js",
    "precommit": "lint-staged",
    "start": "node server.js",
    "test": "npm run format-check && npm run lint"
  },
  "lint-staged": {
    "*.js": [
      "npm run format",
      "git add"
    ],
    "*.css": [
      "npm run format",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IDEA-Hacks/ideahacks.la.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/IDEA-Hacks/ideahacks.la/issues"
  },
  "homepage": "https://github.com/IDEA-Hacks/ideahacks.la#readme",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.17.2",
    "express": "^4.15.3",
    "express-session": "^1.15.5",
    "hbs": "^4.0.1",
    "hbs-utils": "0.0.4",
    "mongoose": "^4.11.3",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "serve-favicon": "^2.4.3",
    "socket.io": "^2.0.3"
  },
  "devDependencies": {
    "eslint": "^4.6.1",
    "eslint-config-prettier": "^2.4.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.4",
    "nodemon": "^1.11.0",
    "prettier": "^1.6.1",
    "prettier-check": "^1.0.0"
  }
}
